<Window x:Class="Company.TwitterStudio.TwitterPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TwitterPanel" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <TextBox Text="{Binding Message}"/>
        <TextBlock Text="{Binding Code}" Grid.Row="1"/>
    </Grid>
</Window>
